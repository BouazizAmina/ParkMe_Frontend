<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".interfaces.Reservation" >

    <EditText
        android:id="@+id/timeA"
        android:layout_width="269dp"
        android:layout_height="58dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="L'heure d'arrivé "
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="325dp" />

    <EditText
        android:id="@+id/timeD"
        android:layout_width="269dp"
        android:layout_height="58dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="L'heure de départ"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="413dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="58dp"
        android:layout_height="60dp"
        android:clickable="true"
        android:src="@android:drawable/arrow_down_float"
        app:backgroundTint="#FFFFFF"
        tools:layout_editor_absoluteX="311dp"
        tools:layout_editor_absoluteY="325dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="329dp"
        android:layout_height="48dp"
        android:layout_marginBottom="92dp"
        android:background="@color/colordarkblue"
        android:backgroundTint="#CD1818"
        android:text="Reserver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeD"
        app:layout_constraintVertical_bias="0.325" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="58dp"
        android:layout_height="60dp"
        android:clickable="true"
        android:src="@android:drawable/arrow_down_float"
        app:backgroundTint="#FFFFFF"
        tools:layout_editor_absoluteX="311dp"
        tools:layout_editor_absoluteY="413dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/park"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-123dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Réservation au parking val d'hydra"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="211dp" />

</androidx.constraintlayout.widget.ConstraintLayout>